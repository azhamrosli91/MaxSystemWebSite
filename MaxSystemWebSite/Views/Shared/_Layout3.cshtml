    <link href="~/css/mobilecss.css" rel="stylesheet" />
    <script src="~/js/mainJS.js"></script>

    <script>
        var phoenixIsRTL = window.config.config.phoenixIsRTL;
        if (phoenixIsRTL) {
          var linkDefault = document.getElementById('style-default');
          var userLinkDefault = document.getElementById('user-style-default');
          linkDefault.setAttribute('disabled', true);
          userLinkDefault.setAttribute('disabled', true);
          document.querySelector('html').setAttribute('dir', 'rtl');
        } else {
          var linkRTL = document.getElementById('style-rtl');
          var userLinkRTL = document.getElementById('user-style-rtl');
          linkRTL.setAttribute('disabled', true);
          userLinkRTL.setAttribute('disabled', true);
        }
    </script>
    <script src="~/lib/select2-4.1.0/js/select2.min.js"></script>
    <link href="~/css/common.css" rel="stylesheet" asp-append-version="true" />

    <style>
        .content {
            height: 100vh;
            padding: 4rem 0 0 0 !important;
        }

        .main-container {
            height: 100%;
            overflow: hidden;
        }

        .main-resizable-container {
            display: flex;
            min-width: 0;
            height: 100%;
            flex: 1;
            position: relative;
        }

        .main-resizable-left {
            display: flex;
            flex-direction: column;
            height: 100%;
            flex-shrink: 0;
            min-width: 400px;
            max-width: 50%;
        }

        .main-resizable-right {
            width: 100%;
            min-width: 0;
        }

        .main-resizer {
            width: 5px;
            background: transparent;
            cursor: col-resize;
            user-select: none;
            position: relative;
            z-index: 2;
        }

            .main-resizer:hover {
                background-color: #0c66e4;
            }

        .main-resizer-ghostline {
            position: absolute;
            top: 0;
            bottom: 0;
            width: 2px;
            background: #0c66e4;
            pointer-events: none;
            display: none;
            z-index: 9999;
        }
    </style>

</head>
